{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "black": "#262B44",
    "blue": "#0c29ce",
    "cyan": "#79f8f8",
    "green": "#59C9A5",
    "navy": "#00897b",
    "orange": "#F07623",
    "red": "#D81E5B",
    "white": "#E0DEF4",
    "yellow": "#fff34a"
  },
  "secondary_prompt": {
    "template": "<p:yellow,transparent>\ue0b6</><,p:yellow> > </><p:yellow,transparent>\ue0b0</> ",
    "foreground": "p:black",
    "background": "transparent"
  },
  "transient_prompt": {
    "style": "plain",
    "template": "\ue0b0 ",
    "foreground": "#D4E157",
    "background": "transparent",
    "type": "exit"
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "folder_icon": "\u2026",
            "folder_separator_icon": " <transparent>\ue0b1</> ",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " \uea83 {{ .Path }} ",
          "foreground": "#f2f3f8",
          "background": "#0087af",
          "type": "path"
        },
        {
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if .IsWorkTree }} \uf1bb{{ .WorktreeCount }}{{ end }}{{ if .StashCount }} *{{ .StashCount }}{{ end }} ",
          "foreground": "p:black",
          "powerline_symbol": "\ue0b0",
          "background": "p:green",
          "type": "git",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:black{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:white{{ end }}",
            "{{ if gt .Ahead 0 }}p:white{{ end }}"
          ],
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}#49416D{{ end }}",
            "{{ if gt .Behind 0 }}#7A306C{{ end }}"
          ]
        },
        {
          "style": "powerline",
          "template": " \uf0e7 ",
          "foreground": "p:black",
          "powerline_symbol": "\ue0b0",
          "background": "p:yellow",
          "type": "root"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:blue",
          "type": "exit",
          "background_templates": ["{{ if gt .Code 0 }}p:red{{ end }}"]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "properties": {
            "display_mode": "files",
            "fetch_package_manager": false,
            "home_enabled": false
          },
          "style": "powerline",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "p:green",
          "powerline_symbol": "\ue0b2",
          "background": "p:blue",
          "type": "node",
          "invert_powerline": true
        },
        {
          "properties": {
            "fetch_version": false
          },
          "style": "powerline",
          "template": "\ue626 ",
          "foreground": "p:blue",
          "powerline_symbol": "\ue0b2",
          "background": "p:blue",
          "type": "go",
          "invert_powerline": true
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": false,
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": "\ue235 ",
          "foreground": "p:yellow",
          "powerline_symbol": "\ue0b2",
          "background": "p:blue",
          "type": "python",
          "invert_powerline": true
        },
        {
          "properties": {
            "style": "round",
            "threshold": 250
          },
          "style": "powerline",
          "template": " {{ .FormattedMs }} \uf252  ",
          "foreground": "p:black",
          "powerline_symbol": "\ue0b2",
          "background": "p:yellow",
          "type": "executiontime",
          "invert_powerline": true
        },
        {
          "style": "powerline",
          "template": "{{ if gt .PhysicalPercentUsed 60 }} {{ round .PhysicalPercentUsed .Precision }}% \ue266 {{ end }}{{ if gt .SwapPercentUsed 0 }} {{ round .SwapPercentUsed .Precision }}% \uebcb{{ end }} {{ if gt .Load1 0 }}{{ .Load1 }} {{ .Load5 }} {{ .Load15 }} \uf063 {{ end }}",
          "foreground": "p:black",
          "powerline_symbol": "\ue0b2",
          "background": "p:orange",
          "type": "sysinfo",
          "min_width": 80,
          "invert_powerline": true
        },
        {
          "style": "powerline",
          "template": " {{ if .SSHSession }}\ueba9 {{ end }}<i>{{ .UserName }}@{{ .HostName }}</i> ",
          "foreground": "p:black",
          "powerline_symbol": "\ue0b2",
          "background": "p:cyan",
          "type": "session",
          "min_width": 80,
          "invert_powerline": true
        },
        {
          "style": "powerline",
          "template": " <p:yellow><b>{{ .Name }}</b></> ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b2",
          "background": "p:navy",
          "type": "shell",
          "min_width": 90,
          "invert_powerline": true
        },
        {
          "style": "powerline",
          "template": "<p:white><b> {{ .CurrentDate | date \"15:04:05 \uf017 \" }}</b></> ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b2",
          "background": "p:blue",
          "type": "time",
          "invert_powerline": true
        },
        {
          "style": "diamond",
          "leading_diamond": "<background,foreground>\ue0b2</>",
          "trailing_diamond": "\ue0b4",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "#000000",
          "background": "#ffffff",
          "type": "os",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "‚ùØ",
          "foreground": "#D4E157",
          "type": "exit",
          "foreground_templates": ["{{ if gt .Code 0 }}#FF5252{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "tooltips": [
    {
      "properties": {
        "display_default": true
      },
      "style": "diamond",
      "leading_diamond": "\ue0b0",
      "trailing_diamond": "\ue0b4",
      "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
      "foreground": "p:white",
      "background": "p:orange",
      "type": "aws",
      "tips": ["aws"]
    },
    {
      "properties": {
        "display_default": true
      },
      "style": "diamond",
      "leading_diamond": "\ue0b0",
      "trailing_diamond": "\ue0b4",
      "template": " \ufd03 {{ .Name }} ",
      "foreground": "p:white",
      "background": "p:blue",
      "type": "az",
      "tips": ["az"]
    }
  ],
  "version": 3,
  "final_space": true
}
